{
    "manifest_version": 3,
    "name": "Teams meeting ver.2",
    "version": "2.0",
    "description": "新しいカレンダー対応",
    "permissions": [
        "activeTab",
        "scripting"
    ],
"content_scripts": [
    {
        "matches": [
        "*://outlook.office.com/*" 
        ],
        "js": ["content.js"],
        "run_at": "document_idle",
        "all_frames": true 
    }
]
}